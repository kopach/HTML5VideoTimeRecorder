{
  "name": "Html5videos",
  "version": "1.0",
  "manifest_version":2,
  "description": "Youtube-like feature to avoid half seen videos",
  "permissions": ["tabs", "<all_urls>","storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.1.1.js","main.js"],
      "all_frames": true
    }
  ]
}
